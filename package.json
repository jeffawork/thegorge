{
  "name": "the-gorge",
  "version": "1.0.0",
  "description": "The Gorge - Real-time blockchain infrastructure monitoring tool",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/app.js",
    "dev": "ts-node src/app.ts",
    "dev:frontend": "cd frontend && npm run dev",
    "build:frontend": "cd frontend && npm run build",
    "preview:frontend": "cd frontend && npm run preview",
    "install:all": "npm install && cd frontend && npm install",
    "setup:db": "node scripts/setup-database.js",
    "migrate": "ts-node scripts/migrate.ts",
    "migrate:dev": "ts-node scripts/dev-migrate.ts",
    "migrate:cli": "ts-node scripts/migration-cli.ts",
    "migrate:create": "ts-node scripts/migration-cli.ts create",
    "migrate:run": "ts-node scripts/run-migrations.ts",
    "seed:run": "ts-node scripts/run-seeds.ts",
    "db": "ts-node scripts/db-cli.ts",
    "db:migrate": "ts-node scripts/db-cli.ts migrate",
    "db:seed": "ts-node scripts/db-cli.ts seed",
    "db:status": "ts-node scripts/db-cli.ts migrate:status",
    "db:connect": "ts-node scripts/db-cli.ts db:connect",
    "setup": "node scripts/setup.js",
    "setup:docker": "node scripts/setup.js -d",
    "setup:clean": "node scripts/setup.js -c",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f",
    "docker:clean": "docker compose down -v",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:unit": "jest --testPathPattern=tests/(services|utils|controllers|middleware)",
    "test:integration": "jest --testPathPattern=tests/integration",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "ethereum",
    "rpc",
    "monitoring",
    "web3",
    "blockchain",
    "react",
    "typescript"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/commander": "^2.12.0",
    "@types/pg": "^8.15.5",
    "@types/uuid": "^10.0.0",
    "bcryptjs": "^3.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "commander": "^14.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "install": "^0.13.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^3.0.3",
    "pg": "^8.16.3",
    "prom-client": "^15.1.3",
    "socket.io": "^4.7.4",
    "uuid": "^11.1.0",
    "web3": "^4.3.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/chokidar": "^2.1.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.19.18",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "chokidar": "^3.6.0",
    "eslint": "^8.54.0",
    "globals": "^16.4.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  }
}
